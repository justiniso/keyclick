{
	"name": "Keyclick",
	"version": "1.0",
	"manifest_version": 2,
	"description": "Ditch your mouse! Click by typing.",
	"permissions": [ "http://*/*", "https://*/*", "tabs"],
	"content_scripts": [{
		"matches": ["http://*/*", "https://*/*"],
		"exclude_matches": ["http://google.com/*", "https://google.com/*", "http://www.google.com/*", "https://www.google.com/*"],
		"js": ["keyclick.js"]
	}]

}